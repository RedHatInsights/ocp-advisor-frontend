{
  "name": "ocp-advisor-frontend",
  "version": "1.17.2",
  "private": false,
  "scripts": {
    "build": "fec build",
    "analyze": "fec build --env analyze=true",
    "lint": "npm-run-all lint:*",
    "lint:js": "eslint config src",
    "lint:js:fix": "eslint config src --fix",
    "lint:sass": "stylelint 'src/**/*.scss' --config .stylelintrc.json",
    "nightly": "npm run build",
    "postinstall": "ts-patch install",
    "start": "fec dev",
    "start:proxy": "PROXY=true fec dev",
    "start:mock": "PROXY=true MOCK=true fec dev",
    "test": "TZ=UTC jest --verbose --passWithNoTests --no-cache",
    "test:coverage": "curl -sSL 'https://raw.githubusercontent.com/RedHatInsights/insights-interact-tools/refs/heads/main/scripts/coverage.sh' | bash",
    "test:ct": "BABEL_ENV=componentTest cypress run --browser chrome --component",
    "test:openct": "BABEL_ENV=component cypress open --component",
    "test:local": "TZ=UTC jest --verbose --collectCoverage=false",
    "translations": "npm-run-all translations:*",
    "translations:extract": "formatjs extract 'src/**/*.js*' --out-file lang/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "translations:compile": "formatjs compile lang/en.json --out-file compiled-lang/en.json",
    "ci:verify": "npm run test:coverage",
    "verify": "npm-run-all build lint test"
  },
  "dependencies": {
    "@patternfly/react-component-groups": "^6.3.0",
    "@patternfly/react-core": "^6.3.1",
    "@patternfly/react-icons": "^6.3.1",
    "@patternfly/react-table": "^6.3.1",
    "@redhat-cloud-services/frontend-components": "^7.0.6",
    "@redhat-cloud-services/frontend-components-advisor-components": "^3.0.18",
    "@redhat-cloud-services/frontend-components-notifications": "^6.1.8",
    "@redhat-cloud-services/frontend-components-translations": "^4.0.13",
    "@redhat-cloud-services/frontend-components-utilities": "^7.0.6",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/webpack-plugin": "^3.2.0",
    "@unleash/proxy-client-react": "^5.0.0",
    "axios": "^1.7.7",
    "lodash": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.8",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.26.1",
    "redux": "^4.2.1",
    "redux-logger": "^3.0.6",
    "semver": "^7.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@cypress/code-coverage": "^3.14.6",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@formatjs/cli": "^6.7.2",
    "@patternfly/patternfly": "^6.3.1",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^3.0.0",
    "@redhat-cloud-services/frontend-components-config": "^6.7.1",
    "@redhat-cloud-services/frontend-components-config-utilities": "^4.7.0",
    "@redhat-cloud-services/tsc-transform-imports": "^1.0.23",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "babel-core": "6.26.3",
    "babel-jest": "^30.2.0",
    "babel-plugin-formatjs": "^10.5.38",
    "babel-plugin-istanbul": "^7.0.0",
    "cypress": "15.3.0",
    "eslint": "9.36.0",
    "eslint-plugin-cypress": "^5.1.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsdoc": "^60.5.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.10.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "redux-mock-store": "^1.5.5",
    "style-loader": "^3.3.4",
    "stylelint": "^16.2.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-scss": "^6.1.0",
    "ts-patch": "^3.1.2",
    "typescript": "^5.4.3",
    "typescript-eslint": "^8.44.0"
  },
  "repository": "git@github.com:RedHatInsights/ocp-advisor-frontend.git",
  "insights": {
    "appname": "ocp-advisor"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!src/**/*.cy.js",
      "!src/**/*.fixtures.js",
      "!src/{entry,entry-dev}.js"
    ],
    "coverageDirectory": "coverage-jest",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(@patternfly/react-core/|@patternfly/react-icons/|@redhat-cloud-services|@openshift|lodash-es|@patternfly/react-table|@patternfly/react-tokens|p-all)).*$"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "nyc": {
    "report-dir": "coverage-cypress",
    "include": [
      "src/**/*.{js,jsx}"
    ],
    "exclude": [
      "src/**/*.test.js",
      "src/**/*.cy.js"
    ]
  }
}
